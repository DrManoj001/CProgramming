#include <stdio.h>

int main()
{
    int i,n,j,k,l;
    scanf("%d",&n);
    i=n;
    while(i>=1)
        {
            j=n;
            while(j>=i)
            {
                printf("%d",j);
                j-=1;
            }
            k=1;
            while(k<(i*2)-1)
            {
                printf("%d",i);
                k+=1;
            }
            l=i+1;
            while(l<=n)
            {
                printf("%d",l);
                l+=1;
            
            }
            i-=1;
            printf("\n");
        
        }
    i=2;
    while(i<=n)
    {
        j=n;
        while(j>=i)
        {
            printf("%d",j);
            j-=1;
        }
    k=1;
    while(k<(i*2)-1)
    {
        printf("%d",i);
        k+=1;
    }
    l=i+1;
    while(l<=n)
    {
        printf("%d",l);
        l+=1;
    }
    
    i+=1;
    printf("\n");
    }
        return 0;
}


